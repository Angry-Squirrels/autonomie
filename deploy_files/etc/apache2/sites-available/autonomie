WSGIApplicationGroup %{GLOBAL}
<VirtualHost *:8080>
    <IfModule mod_rpaf.c>
        RPAFenable On
        RPAFsethostname On
        RPAFproxy_ips 192.168.122.1
    </IfModule>
    ServerName ${app.url}
    <Directory /var/www/autonomie>
    Order allow,deny
    Allow from all
    </Directory>
    
    WSGIDaemonProcess ${app.url} processes=1 threads=15 display-name=%{GROUP} python-path=/var/www/autonomie/lib/python2.6/site-packages
    WSGIProcessGroup ${app.url}
    ## Log Files
    ErrorLog /var/log/apache2/error.log
    CustomLog /var/log/apache2/access.log combined
    WSGIScriptAlias / /var/www/autonomie/pyramid.wsgi
</VirtualHost>
